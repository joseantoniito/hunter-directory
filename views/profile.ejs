            <script type="text/ng-template" id="/direccion.html">
                                                 
            <div class="panel panel-default" >
            <div class="panel-heading" style="background: #00587c">
                <h1 class="panel-title"> <div style="color: #ffffff">Dirección</div></h1>
            </div>
            <div class="panel-body">

                <form class="form-horizontal" role="form">
                                    
                    <div class="form-group">
                        <label class="control-label col-sm-2" for="colonia">Colonia</label>
                        <div class="col-md-5">
                            <input type="email" class="form-control" id="colonia" placeholder="Colonia">
                        </div>
                            <label class="control-label col-sm-1" for="cp">CP:</label>
                        <div class="col-md-2">
                            <input type="email" class="form-control" id="cp" placeholder="cp">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-sm-2" for="calle">Calle</label>
                        <div class="col-md-8">
                            <input type="email" class="form-control" id="calle" placeholder="Calle">
                        </div>   
                    </div>
                    
                    <div class="form-group">
                         <label class="control-label col-sm-2" for="cp">Num. Ext.:</label>
                        <div class="col-sm-3">
                            <input type="email" class="form-control" id="num_ext" placeholder="Número exterior">
                        </div>
                        <label class="control-label col-sm-2    " for="cp">Num. Int.:</label>
                        <div class="col-sm-3">
                            <input type="email" class="form-control" id="num_int" placeholder="Numero interior">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-sm-2" for="phone">Telefono</label>
                        <div class="col-sm-3">
                            <input type="email" class="form-control" id="phone" placeholder="Telefono">
                        </div>
                        <label class="control-label col-sm-2" for="cellphone">Celular</label>
                        <div class="col-sm-3">  
                            <input type="email" class="form-control" id="cellphone" placeholder="Celular">
                        </div>
                    </div>

                    <div class="form-group">
                        <label class="control-label col-sm-2" for="estado">Estado</label>
                        <div class="col-md-4">
                            <select  class="form-control" id="estado" name="Estado" >
                                <option value="">Seleccione una opcion</option>
                                <option value="AGS">Aguascalientes</option>
                                <option value="BC">Baja California</option>
                                <option value="BCS">Baja California Sur</option>
                                <option value="CAMP">Campeche</option>
                                <option value="CHIS">Chiapas</option>
                                <option value="CHIH">Chihuahua</option>
                                <option value="COAH">Coahuila de Zaragoza</option>
                                <option value="COL">Colima</option>
                                <option value="DF">Distrito Federal</option>
                                <option value="DGO">Durango</option>
                                <option value="GTO">Guanajuato</option>
                                <option value="GRO">Guerrero</option>
                                <option value="HGO">Hidalgo</option>
                                <option value="JAL">Jalisco</option>
                                <option value="MEX">México</option>
                                <option value="MICH">Michoacán de Ocampo</option>
                                <option value="MOR">Morelos</option>
                                <option value="NAY">Nayarit</option>
                                <option value="NL">Nuevo León</option>
                                <option value="OAX">Oaxaca</option>
                                <option value="PUE">Puebla</option>
                                <option value="QRO">Querétaro</option>
                                <option value="QR">Quintana Roo</option>
                                <option value="SLP">San Luis Potosí</option>
                                <option value="SIN">Sinaloa</option>
                                <option value="SON">Sonora</option>
                                <option value="TAB">Tabasco</option>
                                <option value="TAMS">Tamaulipas</option>
                                <option value="TLAX">Tlaxcala</option>
                                <option value="VER">Veracruz de Ignacio de la Llave</option>
                                <option value="YUC">Yucatán</option>
                                <option value="ZAC">Zacatecas</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2" >Centrar en el mapa</label>
                        <div class="col-sm-2">
                            <button type="submit" class="btn btn-lg btn-info" >Geolocalizar</button>
                        </div>
                        <div class="col-md-9" id="map"></div>       
                    </div>
                    <div class="form-group">
                        <div class="col-sm-offset-10 col-sm-10">
                            <button type="submit" class="btn btn-lg btn-warning" >Registrar</button>
                        </div>

                    </div>
                </form>
            </div>
        </div>           
                        <script>
                            var map;
                            function initMap() {
                                map = new google.maps.Map(document.getElementById('map'), {
                                    center: {lat: 20.595679, lng: -100.405836},
                                    zoom: 8
                                });
                            }
                        </script>                      
                        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA8Xq5o6lNl4YyFt0F30E_yas-zDNk3sMg&callback=initMap" async defer></script> 
            </script>
        </div>
          
          <script type="text/ng-template" id="/suscripcion.html">
            <div class="col-md-6 col-md-offset-2 bgrdWhite">
                <div class="page-header" >
                    <h1 class="lblAzulObscuro">Suscripción</h1>
                </div>
                <div ng-show="error" class="alert alert-danger row">
                    <span>"Error no found"</span>
                </div>

                <form ng-submit="logIn()" class="form-horizontal m30">
                    <div class="form-group">
                        <input type="text" class="form-control"   placeholder="Número de tarjeta"></input>
                    </div>
            
                    <div class="form-group">
                        <input type="password"    class="form-control"      placeholder="Código de seguridad"></input>
                    </div>
          <div class="form-group">
                        <input type="password"    class="form-control"      placeholder="Fecha de "></input>
                    </div>
                        <button type="submit" class="btn btn-primary">Iniciar Sesión</button>
                </form>
            </div>
          </script>
          
        <script type="text/ng-template" id="/editProfile.html">
            <div class="col-md-6 col-md-offset-1 bgrdWhite">
                <div class="page-header" >
                    <h1 class="lblAzulObscuro">Editar Profile</h1>
                </div>
                <div ng-show="error" class="alert alert-danger row">
                    <span>"Error no found"</span>
                </div>

          
                <form ng-submit="editProfile()" class="m30">
                    
               <a href="#" class="thumbnail">
                       <img src="data:image/svg+xml;base64," class="img-rounded" alt="Icono empresa" width="304" height="236">
                    </a>
                    <div class="form-group">
          
                       <label class="control-label col-sm-2">Nombre</label>
                        <input type="text" class="form-control"   placeholder="nombre"></input>
                    </div>
          
                    <div class="form-group">
          
                        <label class="control-label col-sm-2">Password</label>
                        <input type="password"    class="form-control"      placeholder="Password"></input>
                    </div>
                    <div class="form-group">
                        <label class="control-label col-sm-2">Email</label>
                        <input type="password"    class="form-control"      placeholder="Email"></input>
                    </div>
                    
                    <div class="form-group">
                        <label class="control-label col-sm-2">Email</label>
                        <input type="email"    class="form-control"      placeholder="Email"></input>
                    </div>
                      <button type="submit" class="btn btn-primary">Iniciar Sesión</button>
                </form>
            </div>
          </script>
          
          
          <script type="text/ng-template" id="/notificaciones.html">
            <div class="col-md-6 bgrdWhite">
              <div class="page-header" >
                    <h1 class="lblAzulObscuro">Notificaciones</h1>
                </div>
                <form class="form-horizontal">
                    <div class="form-group">
                        <label class="control-label col-sm-4">Comentarios</label>
                        <div class="col-md-offset-8">
                            <input type="checkbox" id="notifications-switch" checked="checked" />
                        </div>
                    </div>
          
                    <div class="form-group">
                        <label class="control-label col-sm-4">Nuevo seguidor</label>
                        <div class="col-md-offset-8">
                            <input type="checkbox" id="mail-switch"  checked="checked" />
                        </div>
                    </div>
                
                </form>
                </div>

          </script>
    </row>
</div>
    <script>
    $(function() {
        $("#notifications-switch").kendoMobileSwitch();
        $("#mail-switch").kendoMobileSwitch({
            onLabel: "YES",
            offLabel: "NO"
        });
        $("#visible-switch").kendoMobileSwitch({
            checked: true
        });
        $("#name-switch").kendoMobileSwitch();
    });
</script>

                   
        
  </body>
    
    
